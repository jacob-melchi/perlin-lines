{
    "files.associations": {
        "cairo.h": "c",
        "cmath": "cpp",
        "stdio.h": "c",
        "complex": "cpp",
        "bitset": "cpp",
        "settings.h": "c",
        "vector.h": "c",
        "vector": "cpp",
        "iosfwd": "cpp",
        "iterator": "cpp",
        "stdbool.h": "c",
        "*.tcc": "c",
        "string": "c",
        "string_view": "c",
        "__locale": "c",
        "__string": "c"
    },
    "runOnSave.statusMessageTimeout": 3000,
    "runOnSave.commands": [
        {
            "match": ".*\\.c$",
            "command": "make -f \"${workspaceFolder}/MakefileCppUTest.mk\"",
            "runIn": "terminal"
        },
        {
            "match": ".*\\.h$",
            "command": "make -f \"${workspaceFolder}/MakefileCppUTest.mk\"",
            "runIn": "terminal"
        },
        {
            "match": ".*\\.cpp$",
            "command": "make -f \"${workspaceFolder}/MakefileCppUTest.mk\"",
            "runIn": "terminal"
        },
    ],
    "makefile.configureOnOpen": false
}